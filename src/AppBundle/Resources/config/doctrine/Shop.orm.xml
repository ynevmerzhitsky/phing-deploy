<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Shop" table="shop">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idPlace" type="bigint" column="id_place" nullable="false"/>
    <field name="rating" type="boolean" column="rating" nullable="true"/>
    <field name="assortment" type="integer" column="assortment" nullable="false"/>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="additionalInformation" type="string" column="additional_information" length="120" nullable="true"/>
  </entity>
</doctrine-mapping>
