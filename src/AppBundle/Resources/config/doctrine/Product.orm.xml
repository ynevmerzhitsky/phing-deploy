<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Product" table="product">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idCategory" type="integer" column="id_category" nullable="false"/>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="brand" type="string" column="brand" length="70" nullable="false"/>
    <field name="barCode" type="string" column="bar_code" length="15" nullable="true"/>
  </entity>
</doctrine-mapping>
